@model Satinal
<form method="post">
    <input asp-for="Id" hidden />
    <div class="border p-3 mt-5">

        <div class="row pb-3">
            <h2 class="text-primary">satın alma işlemini sil</h2>
            <hr/>
        </div>
        <div class="mb-3">
            <label asp-for="OgrenciId" class="p-0"></label>
            <input asp-for="OgrenciId" class="form-control" />
        </div>

         <div class="mb-3">
            <label asp-for="KursId" class="p-0"></label>
            <select asp-for="KursId" asp-items="ViewBag.KitapList" class="form-select">
            </select>
        </div>

        
        <div>
            <button type="submit" class="btn btn-primary" style="width:250px">satın alma kayıt sil</button>
            <a asp-controller="Satinal" asp-action="Index" class="btn btn-primary" style="width:250px">satın alma listesine geri dön</a>
        </div>

    </div>
</form>

